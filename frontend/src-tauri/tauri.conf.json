{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "contacts",
  "version": "0.1.0",
  "identifier": "com.GiRRaFFee.contactsapp",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build"
  },
  "app": {
    "windows": [
      {
        "title": "contacts",
        "width": 1280,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/icon.ico"]
  }
}
